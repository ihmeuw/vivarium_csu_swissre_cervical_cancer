components:
    vivarium_public_health:
        population:
            - BasePopulation()
            - Mortality()

    vivarium_csu_swissre_cervical_cancer.components:
        - CervicalCancer()
        - ScreeningAlgorithm()
        - MortalityObserver()
        - DisabilityObserver()
        - StateMachineObserver('cervical_cancer')
        - StateMachineObserver('screening_result', 'False')

        - ScreeningObserver()

configuration:
    input_data:
        location: {{ location_proper }}
        input_draw_number: 0
        artifact_path: {{ artifact_directory }}/{{ location_sanitized }}.hdf
        # XXX for local MacBook:::
        # artifact_path: /Volumes/costeffectiveness/artifacts/vivarium_csu_swissre_cervical_cancer/{{ location_sanitized }}.hdf
    interpolation:
        order: 0
        extrapolate: True
    randomness:
        map_size: 1_000_000
        key_columns: ['entrance_time', 'age']
        random_seed: 0
    time:
        start:
            year: 2020
            month: 1
            day: 1
        end:
            year: 2040
            month: 12
            day: 31
        step_size: 36.5 # Days
    population:
        population_size: 200000
        age_start: 15
        age_end: 95
        # need to include 95+ age_bin

    metrics:
        disability:
            by_age: False
            by_sex: False
            by_year: True
        mortality:
            by_age: False
            by_sex: False
            by_year: True
        cervical_cancer:
            by_age: False
            by_sex: False
            by_year: True
